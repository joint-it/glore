{
  "name": "@jointorg/prettier-config",
  "version": "0.0.0",
  "type": "module",
  "main": "./build/index.js",
  "prettier": "./build/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "check": "pnpm run typecheck && pnpm run lint"
  },
  "devDependencies": {
    "prettier": "catalog:"
  }
}
